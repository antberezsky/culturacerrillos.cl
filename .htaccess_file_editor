# invoke rewrite engine
RewriteEngine On
RewriteBase /scraping/files/demo/culturacerrillos.cl_elenanowak712_gmail.com_yqa9g82aoo/

Options -Indexes
<IfModule mod_negotiation.c>
	Options -MultiViews
</IfModule>

# Prevent scraping from archive for free demo
RewriteCond %{HTTP_USER_AGENT} (ia_archiver) [NC]
RewriteRule . - [F,L]

# Specific redirections
RewriteRule ^index\.html$ /scraping/files/demo/culturacerrillos.cl_elenanowak712_gmail.com_yqa9g82aoo/ [L,R=301]
